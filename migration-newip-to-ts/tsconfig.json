{
  "compilerOptions": {
    "outDir": "./dist",
    "module": "es6",
    "target": "es6",
    "sourceMap": true,
    "skipLibCheck": true,
    "noImplicitAny": true,
    "strictNullChecks": true,
    "strictPropertyInitialization": true,
    "strictFunctionTypes": true,
    "noImplicitThis": true,
    "noPropertyAccessFromIndexSignature": true,
  },
  "include": ["./src"],
}