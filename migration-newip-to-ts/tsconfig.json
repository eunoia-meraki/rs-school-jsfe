{
  "compilerOptions": {
    "outDir": "./dist",
    "module": "es6",
    "target": "es6",
    "moduleResolution": "node",
    "baseUrl": "src",
    "paths": {
      "interfaces": ["./components/interfaces/interfaces"]
    },
    "sourceMap": true,
    "skipLibCheck": true,
    "noImplicitAny": true,
    "strictNullChecks": true,
    "strictPropertyInitialization": true,
    "strictFunctionTypes": true,
    "noImplicitThis": true,
    "noPropertyAccessFromIndexSignature": true,
  },
  "include": ["./src"],
}